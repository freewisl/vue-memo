{"ast":null,"code":"import { reactive } from 'vue';\nimport axios from 'axios';\nexport default {\n  setup() {\n    const state = reactive({\n      data: []\n    });\n\n    const add = () => {\n      const content = prompt('내용을 입력해주세요.');\n\n      if (!content) {\n        alert('내용을 입력해주세요.');\n        return add();\n      }\n\n      axios.post('/api/memos', {\n        content\n      }).then(res => {\n        state.data = res.data;\n      });\n    };\n\n    const edit = id => {\n      const content = prompt('내용을 입력해주세요', state.data[id]);\n      console.log(content);\n      axios.put('/api/memos/' + id, {\n        content\n      }).then(res => {\n        state.data = res.data;\n      });\n    };\n\n    axios.get('/api/memos').then(res => {\n      state.data = res.data;\n    });\n    return {\n      state,\n      add,\n      edit\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAWA,SAASA,QAAT,QAAyB,KAAzB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,eAAe;EACbC,KAAK,GAAG;IACN,MAAMC,KAAI,GAAIH,QAAQ,CAAC;MACrBI,IAAI,EAAE;IADe,CAAD,CAAtB;;IAIA,MAAMC,GAAE,GAAI,MAAM;MAChB,MAAMC,OAAM,GAAIC,MAAM,CAAC,aAAD,CAAtB;;MAEA,IAAI,CAACD,OAAL,EAAc;QACZE,KAAK,CAAC,aAAD,CAAL;QACA,OAAOH,GAAG,EAAV;MACF;;MAEAJ,KAAK,CAACQ,IAAN,CAAW,YAAX,EAAyB;QAAEH;MAAF,CAAzB,EAAsCI,IAAtC,CAA4CC,GAAD,IAAS;QAClDR,KAAK,CAACC,IAAN,GAAaO,GAAG,CAACP,IAAjB;MACD,CAFD;IAGF,CAXA;;IAaA,MAAMQ,IAAG,GAAKC,EAAD,IAAQ;MACnB,MAAMP,OAAM,GAAIC,MAAM,CAAC,YAAD,EAAeJ,KAAK,CAACC,IAAN,CAAWS,EAAX,CAAf,CAAtB;MACAC,OAAO,CAACC,GAAR,CAAYT,OAAZ;MACAL,KAAK,CAACe,GAAN,CAAU,gBAAgBH,EAA1B,EAA8B;QAAEP;MAAF,CAA9B,EAA2CI,IAA3C,CAAiDC,GAAD,IAAS;QACvDR,KAAK,CAACC,IAAN,GAAaO,GAAG,CAACP,IAAjB;MACD,CAFD;IAGF,CANA;;IAQAH,KAAK,CAACgB,GAAN,CAAU,YAAV,EAAwBP,IAAxB,CAA8BC,GAAD,IAAS;MACpCR,KAAK,CAACC,IAAN,GAAaO,GAAG,CAACP,IAAjB;IACD,CAFD;IAIA,OAAO;MAAED,KAAF;MAASE,GAAT;MAAcO;IAAd,CAAP;EACF;;AAhCa,CAAf","names":["reactive","axios","setup","state","data","add","content","prompt","alert","post","then","res","edit","id","console","log","put","get"],"sourceRoot":"","sources":["C:\\workspace\\vue\\vue-memo\\frontend\\src\\components\\Memo.vue"],"sourcesContent":["<template>\r\n  <div class=\"memo\">\r\n    <div class=\"act\">\r\n      <button class=\"btn btn-primary\" @click=\"add()\">+ 추가</button>\r\n    </div>\r\n    <ul>\r\n      <li v-for=\"d in state.data\" :key=\"d.id\" @click=\"edit(d.id)\">{{ d.content }}</li>\r\n    </ul>\r\n  </div>\r\n</template>\r\n<script>\r\nimport { reactive } from 'vue'\r\nimport axios from 'axios'\r\nexport default {\r\n  setup() {\r\n    const state = reactive({\r\n      data: []\r\n    })\r\n\r\n    const add = () => {\r\n      const content = prompt('내용을 입력해주세요.')\r\n\r\n      if (!content) {\r\n        alert('내용을 입력해주세요.')\r\n        return add()\r\n      }\r\n\r\n      axios.post('/api/memos', { content }).then((res) => {\r\n        state.data = res.data\r\n      })\r\n    }\r\n\r\n    const edit = (id) => {\r\n      const content = prompt('내용을 입력해주세요', state.data[id])\r\n      console.log(content)\r\n      axios.put('/api/memos/' + id, { content }).then((res) => {\r\n        state.data = res.data\r\n      })\r\n    }\r\n\r\n    axios.get('/api/memos').then((res) => {\r\n      state.data = res.data\r\n    })\r\n\r\n    return { state, add, edit }\r\n  }\r\n}\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n.memo {\r\n  .act {\r\n    padding: 10px 10px 5px 5px;\r\n    text-align: right;\r\n  }\r\n  ul {\r\n    list-style: none;\r\n    padding: 15px;\r\n    margin: 0;\r\n\r\n    li {\r\n      padding: 15px;\r\n      margin: 10px 0;\r\n      border: 1px solid #eee;\r\n    }\r\n  }\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}